    },
    {
      "column_": {
        "id_hash_": "SNT_VT300520_001000",
        "text_": "伊度，要投降了嗎？"
      }
    },
    {
      "column_": {
        "id_hash_": "SNT_VT300520_002000",
        "text_": "伊度，要投降了嗎？"
