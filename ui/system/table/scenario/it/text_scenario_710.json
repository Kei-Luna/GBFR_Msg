      }
    },
    {
      "column_": {
        "id_hash_": "SNT_CT009020_0370",
        "text_": ""
      }
    },
    {
      "column_": {
        "id_hash_": "SNT_CT009020_0380",
        "text_": ""
      }
    },
    {
      "column_": {
        "id_hash_": "SNT_CT009020_0390",
        "text_": "Dove sarà Rolan..."
      }
    },
    {
      "column_": {
        "id_hash_": "SNT_CT009020_0400",
        "text_": ""
      }
    },
    {
      "column_": {
